#include<bits/stdc++.h>
using namespace std;

//Time Complexity - O(logn)
//Space Complexity - O(2*n)

vector<int>seg;

int rangeSum(int start, int end, int i, int l, int r){
    //case 1 - agar start aur end range ke bahar hai to
    if(l > end || r < start) {
        return 0;
    }
    //Case 2- agar start aur end range ke andar hai to  
    if(l >= start && r <= end) {
        return seg[i];
    }
    //case 3- agar overlapping hai to
    
    int mid = l + (r-l)/2;
    
    return rangeSum(start, end, 2*i+1, l, mid) + rangeSum(start, end, 2*i+2, mid+1, r);
}


void buildTree(int i, vector<int>&nums, int l, int r){
    if(l == r){
        seg[i] = nums[l];
        return;
    }
    int mid = l+(r-l)/2;
    
    buildTree(2*i+1, nums, l, mid);
    buildTree(2*i+2, nums, mid+1, r);
    
    seg[i] = seg[2*i+1]+seg[2*i+2];
}


int main(){
    string input;
    getline(cin, input);
    stringstream ss(input);
    string token;
    vector<int>nums;
    while(getline(ss, token, ' ')){
        nums.push_back(stoi(token));
    }
    
    int n = nums.size();
    seg.resize(2*n-1);
    buildTree(0, nums,0, n-1);
    
    int q1 = 2, q2 = 4;
    
    cout<<rangeSum(q1, q2,0 , 0, n-1);
    
    return 0;
    
}
